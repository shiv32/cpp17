{
  "cmake.buildType": "Debug",
  "cmake.debugConfig": {
    "type": "cppdbg",
    "MIMode": "gdb",
    "miDebuggerPath": "/usr/bin/gdb",
    "externalConsole": false,
    "stopAtEntry": false,
    "cwd": "${workspaceFolder}",
    "setupCommands": [
      {
        "description": "Enable pretty-printing",
        "text": "-enable-pretty-printing",
        "ignoreFailures": true
      }
    ],
    "skipFiles": [
      "**/bits/**",
      "**/c++/**",
      "**/include/**",
      "**/__gnu_cxx/**",
      "**/libstdc++/**",
      "**/usr/include/**",
      "<*>"
    ]
  }
}
